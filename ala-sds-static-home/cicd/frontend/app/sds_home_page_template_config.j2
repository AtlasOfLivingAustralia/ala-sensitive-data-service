    "pDashboardBucketPath" : "{{ dashboard_bucket_path }}",
    "pDashboardSubDomain"  : "{{ dashboard_sub_domain }}"
    "pEnvironment"         : "{{ environment }}",
    "pHostedZone"          : "{{ hosted_zone }}",
    "pProductName"         : "{{ product_name }}",
    "pRegionsBucketPath"   : "{{ regions_bucket_path }}",
    "pRegionsSubDomain"    : "{{ regions_sub_domain }}"
    "pSourceBucket"        : "{{ source_bucket }}",
    "pSslCertificate"      : "{{ ssl_certificate }}",
  },
  "Tags" : {
    "product"     : "{{ product_name }}",
    "component"   : "{{ product_component }}",
    "environment" : "{{ environment }}",
    "branch"      : "{{ src_branch }}",
    "version"     : "{{ commit_id }}",
